    "pluginsConfig": {
    	"change_girls" : {
            "time" : 60,
            "urls" : [
                "https://gitee.com/gaoxiang15125/pictureBed/raw/master/img/beauty1.png",
                "https://gitee.com/gaoxiang15125/pictureBed/raw/master/img/beauty2.png",
                "https://gitee.com/gaoxiang15125/pictureBed/raw/master/img/beauty3.png",
