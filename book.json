        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy IT小菜鸡搬运社 2023",
            "modify_label": "文章修订时间：",
            "modify_format": "YYYY-MM-DD"
        },
        "insert-logo": {
            "url": "images/logo.ico",
            "style": "background: none; max-height: 100px; min-height: 100px"
        },
        "flexible-alerts": {
          "style": "flat",
          "note": {
            "label": "重点"
          },
          "tip": {
            "label": "建议"
          },
          "warning": {
            "label": "注意事项"
          },
          "danger": {
            "label": "危险"
          },
        "question": {
            "label": "问题",
            "icon": "fa fa-question-circle",
            "className": "info"
        },
        "comment": {
            "label": "评论",
            "icon": "fa fa-comments",
            "className": "info"
        }
    }
    }
